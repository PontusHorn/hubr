{
    "name": "PontusHorn/hubr",
    "description": "An application to find a Github username and user ID from a Github URL",
    "license": "MIT",
    "authors": [
        {
            "name": "Pontus Horn",
            "email": "pontus.horn@gmail.com",
            "homepage": "https://pontushorn.me/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "slim/twig-view": "^2.1",
        "willdurand/negotiation": "^2.0",
        "rmccue/requests": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {"PontusHorn\\": "src/"},
        "exclude-from-classmap": ["/tests/"]
    },
    "autoload-dev": {
        "psr-4": {"PontusHorn\\Tests\\": "tests/"}
    }
}
